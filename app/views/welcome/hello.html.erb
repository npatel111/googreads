Welcome to googreads!
Hi <%= @current_user ? @current_user.name : 'stranger' %>
<br>

<% if @current_user %>
  <a href="/users/<%=@current_user.id%>">Go to my page</a>

  <%= form_tag logout_path do %>
  <%= submit_tag "Logout" %>
  <% end %>
<% else %>
  <%= link_to "Sign Up", new_user_path %>
  <br>
  <%= link_to "Log In", login_path %>
<% end %>
