Rating: <%=@review.rating%><br>
Content: <%=@review.content%><br>
Book: <%=@review.book.title%><br>
<%if @review.user_id == session["user_id"]%>
  <a href="/reviews/<%=@review.id%>/edit">Edit this rating</a>
  <%= form_tag review_path(@review), method: "delete" do%>
    <%= submit_tag "Delete review" %>
  <% end %>
<%end%>
<br>
<%= link_to "Home", root_path %>
